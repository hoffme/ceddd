{
  "name": "ceddd",
  "version": "0.1.1",
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./app": "./lib/app/index.js",
    "./commands": "./lib/commands/index.js",
    "./ctx": "./lib/ctx/index.js",
    "./error": "./lib/error/index.js",
    "./events": "./lib/events/index.js",
    "./infrastructure": "./lib/infrastructure/index.js",
    "./resolver": "./lib/resolver/index.js",
    "./shared": "./lib/shared/index.js"
  },
  "typesVersions": {
    "*": {
      "app": ["lib/app/index.d.ts"],
      "commands": ["lib/commands/index.d.ts"],
      "ctx": ["lib/ctx/index.d.ts"],
      "error": ["lib/error/index.d.ts"],
      "events": ["lib/events/index.d.ts"],
      "infrastructure": ["lib/infrastructure/index.d.ts"],
      "resolver": ["lib/resolver/index.d.ts"],
      "shared": ["lib/shared/index.d.ts"]
    }
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "lint": "npx eslint src",
    "lint:fix": "npx lint -- --fix",
    "format": "npx prettier src --check",
    "format:fix": "npx prettier -- --write",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "author": "hoffme",
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoffme/ceddd.git"
  },
  "bugs": {
    "url": "https://github.com/hoffme/ceddd/issues"
  }
}
